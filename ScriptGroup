local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Script Group", HidePremium = false, IntroText = "Script Group", SaveConfig = true, ConfigFolder = "ScriptSquad"})

